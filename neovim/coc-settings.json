{
  "coc.preferences.maxFileSize": "1MB",
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "inlayHint.enable": false,
  "signature.target": "echo",
  "suggest.timeout": 7000,
  "suggest.noselect": true,
  "suggest.labelMaxLength": 100,
  "codeLens.enable": false,
  "colors.filetypes": ["*"],
  "diagnostic.virtualText": false,
  "diagnostic.enableMessage": "jump",
  "yank.enableCompletion": false,
  "list.indicator": "Î»",
  "list.interactiveDebounceTime": 300,
  "list.limitLines": 300,
  "list.normalMappings": {},
  "list.source.grep.excludePatterns": ["**/*.min.js", "**/*.min.css", "**/*.js.map"],
  "coc.source.lines.enable": false,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "snippets.textmateSnippetsRoots": ["~/.dot-files/neovim/snippets"],
  "snippets.extends": {
    "cpp": ["c"],
    "typescript": ["javascript"],
    "typescriptreact": ["typescript"]
  },
  "snippets.priority": 0,
  "html.filetypes": ["html", "handlebars", "htmldjango", "blade", "eelixir"],
  "html.autoCreateQuotes": false,
  "css.lint.unknownAtRules": "ignore",
  "css.completion.completePropertyWithSemicolon": false,
  "scss.completion.completePropertyWithSemicolon": false,
  "less.completion.completePropertyWithSemicolon": false,
  "cssmodules.camelCase": true,
  "cssmodules.hintName": "CM",
  "typescript.suggest.includeCompletionsWithSnippetText": false,
  "typescript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,
  "highlight.disableLanguages": ["dirvish"],
  "svelte.enable-ts-plugin": true,
  "rust-analyzer.server.path": "rust-analyzer",
  "rust-analyzer.diagnostics.enable": false,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.lens.enable": false,
  "Lua.telemetry.enable": false,
  "Lua.diagnostics.enable": false,
  "Lua.diagnostics.globals": ["love"],
  "Lua.diagnostics.disable": ["lowercase-global"],
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "pyright.disableDiagnostics": true,
  "clangd.enabled": true,
  "clangd.disableDiagnostics": true,
  "clangd.arguments": ["--background-index", "--completion-style=detailed"],
  "languageserver": {
    "elixirLS": {
      "command": "/opt/homebrew/bin/elixir-ls",
      "disabledFeatures": ["inlayHint"],
      "settings": {
        "suggestSpecs": false,
        "dialyzerEnabled": true,
        "signatureAfterComplete": false,
        "fetchDeps": false
      },
      "filetypes": ["elixir", "eelixir"]
    },
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript", "gdscript3"],
      "port": 6005
    },
    "zig": {
      "command": "zls",
      "filetypes": ["zig"]
    }
  }
}
