{
  "diagnostic.errorSign": "E",
  "diagnostic.warningSign": "W",
  "diagnostic.infoSign": "I",
  "diagnostic.hintSign": "H",
  "suggest.timeout": 2000,
  "suggest.enablePreview": true,
  "suggest.echodocSupport": true,
  "signature.target": "echo",
  "codeLens.enable": true,
  "diagnostic.virtualText": false,
  "list.indicator": "Î»",
  "list.interactiveDebounceTime": 1000,
  "list.limitLines": 300,
  "list.normalMappings": {},
  "list.source.grep.excludePatterns": [
    "**/*.min.js",
    "**/*.min.css",
    "**/*.js.map"
  ],
  "highlight.disableLanguages": ["dirvish"],
  "coc.source.omni.filetypes": ["cmake"],
  "rust-analyzer.server.path": "rust-analyzer",
  "rust-analyzer.lens.enable": false,
  "Lua.diagnostics.globals": ["love"],
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "languageserver": {
    "godot": {
      "host": "127.0.0.1",
      "filetypes": ["gdscript", "gdscript3"],
      "port": 6008
    }
  }
}
